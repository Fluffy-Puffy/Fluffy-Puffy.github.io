{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/Fluffy-Puffy/homework10.github.io/main/Unemployment.csv"},
    "mark": "point",
    "width":"container",
    "height": {"step": 12},
    "encoding": {
      "facet": {
        "field": "income_level",
        "type": "ordinal",
        "columns": 2,
        "sort": {"op": "mean", "field":"Population growth (annual %)"}
      },
      "x": {
        "aggregate": "mean",
        "field": "Population growth (annual %)",
        "type": "quantitative",
        "scale": {"zero": true}
      },
      "y": {
        "field": "region",
        "type": "ordinal",
        "sort": "-x"
      },
      "color": {"field": "date", "type": "nominal"}
    }
  }