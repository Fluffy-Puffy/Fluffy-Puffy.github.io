{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/Fluffy-Puffy/Fluffy-Puffy.github.io/main/Homework%20week%2010/Unemployment.csv"},
  
    "mark": "line",
    "width": {"step": 50},
    "encoding": {
      "x": {
        "field": "date",
        "type": "ordinal",
        "scale": {"padding": 0.5}
      },
      "y": {
        "aggregate": "mean",
        "field": "Children out of school (% of primary school age)",
        "type": "quantitative"
      },
      "color": {"field": "region", "type": "nominal"}
    }
  }