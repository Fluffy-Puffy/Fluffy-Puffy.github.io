{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "data": {"url": "https://raw.githubusercontent.com/Fluffy-Puffy/Fluffy-Puffy.github.io/main/Assignment_2/Unemployment.csv"},
    "mark": "bar",
    "encoding": {
      "column": {"field": "income_level"},
